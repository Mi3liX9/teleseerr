  },
  "dependencies": {
    "@grammyjs/conversations": "^1.2.0",
    "@grammyjs/hydrate": "^1.4.1",
    "drizzle-orm": "^0.31.1",
    "grammy": "^1.24.1",
    "hono": "^4.4.7",
    "zod": "^3.23.8"
  }
}
